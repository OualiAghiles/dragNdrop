{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./docs",
    "encoding": "utf8",
    "readme": "./README.md",
    "recurse": true,
    "verbose": true,
    "tutorials": "./tutorials",
    "template": "node_modules/foodoc/template"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "name": "Nassidjcom",
    "cleverLinks": false,
    "monospaceLinks": false,
    "systemColor": "#BB2031",
    "default": {
      "outputSourceFiles": true
    },
    "path": "ink-docstrap",
    "navType": "vertical",
    "linenum": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a",
    "footerText": "NSC Lib"

  }
}
