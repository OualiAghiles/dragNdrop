{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./docs/gen",
    "encoding": "utf8",
    "readme": "./README.md",
    "recurse": true,
    "verbose": true,
    "tutorials": "./tutorials",
    "template": "node_modules/better-docs"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "path": "ink-docstrap",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"

  }
}
